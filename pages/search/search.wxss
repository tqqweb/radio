.bacbox{
  width:100%;
  min-height:100%;
  height:100%;
  position: fixed; 
  top:0;left:0; 
  background: linear-gradient(to bottom, #1280D7 0%,#72B2E0 50%,#486682 100%);
}
.search{
  width:600rpx;
  height:100rpx;
  margin:0 auto;
  position: absolute;
  top:50rpx;
  left:75rpx;
  border:1px solid #eee;
  border-radius:50rpx;
  overflow: hidden;
}
.txt{
  width:460rpx;
  height:100rpx;
  float:left;
  padding-left:40rpx;
  font-size: 32rpx;
  line-height: 100rpx;
  color:#fff;
}
.btn{
  width:100rpx;
  height:100rpx;
  border:0;
  line-height: 100rpx;
  font-size: 24rpx;
  color:#ccc;
  border-radius:50rpx;
   /* background: url(../../img/s1.png) no-repeat center;  */
}

.seacont{
  width:600rpx;
  height:600rpx;
  position: absolute;
  top:200rpx;
  left:75rpx;
  overflow: hidden;
}
.inner{
  width:100%;
  height:50rpx;
  font-size: 24rpx;
  line-height: 50rpx;
  color:#fff;
  padding: 10rpx auto;
}
.inner navigator{
  width: 100%;
  height: 100%;
}
.inner text{
  color:#fff;
}
.ti{
  float: left;
}
.au{
  float: right;
}