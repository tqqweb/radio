.playerbox{
  width:200rpx;
  height:290rpx;
  float:left;
  border-radius: 15rpx;
  margin:23rpx;
  text-align: center;
  background: #55BBBD;
  border: 1rpx solid #55BBBD;
}
.playerbox image{
  width:200rpx;
  height:200rpx;
  display: block; 
  border-radius: 15rpx;
}
.playerbox text{
  display: block;
  width:100%;
  height:30rpx;
  text-align: center;
  /* font-size: 20rpx; */
  padding: 10rpx 0;
  color: #fff;
}
.playerbox .name1{
  font-size: 30rpx;
   line-height: 25rpx;
}
.playerbox .coun{
  font-size: 20rpx;
   line-height:25rpx;
}
.zm{
  width:20rpx;
  height:100%;
  position: fixed;
  left:0;
  top:0;
  font-size: 20rpx;
  line-height:40rpx;
  background:#fff;
  word-break:break-all;
}
